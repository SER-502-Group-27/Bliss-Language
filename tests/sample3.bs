def Fibonacci(n):
    if n < 0:
        result = "Invalid input"
    else:
        if n == 0:
            result = 0
        if n == 1:
            result = 1
        if n > 1:
            a = 0
            b = 1
            for i in range(2, n + 1):
                c = a + b
                a = b
                b = c
            result = b

    print(result)

Fibonacci(7)
